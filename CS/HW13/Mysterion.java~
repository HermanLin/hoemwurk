public class Mysterion {

    public int mysterion (int[][] arr, int a, int b, int c) {
	int v = arr[c];
	int store = arr[c];
	arr[c] = arr[b];
	arr[b] = store;
	s = a;
	for (int i = a; i < b-1; i ++) {
	    if (arr[i] < v) {
		store = arr[s];
		arr[s] = arr[i];
		arr[i] = store;
		s += 1;
	    }	    
	}
	swap(b, s);
	return s;
    }
}
